{
  "name": "nemerichun/laravel-szamlazzhu",
  "license": "MIT",
  "description": "Szamlazz.hu integration for Laravel",
  "version": "1.0.1",
  "type": "library",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "download",
    "pdf",
    "xml",
    "billing",
    "szamlazz.hu",
    "szamlazzhu",
    "currency"
  ],
  "authors": [
    {
      "name": "Nemeric",
      "email": "nemeric@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1.3",
    "ext-simplexml": "*",
    "ext-xmlwriter": "*",
    "guzzlehttp/guzzle": "6.*",
    "laravel/framework": ">5.6 | 6.* | 7.* | 8.*"
  },
  "require-dev": {
    "orchestra/testbench": "*",
    "phpunit/phpunit": "*",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "nemerichun\\szamlazzhu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "nemerichun\\szamlazzhu\\Tests\\": "test/"
    }
  },
  "suggest": {
    "illuminate/validation": "5.7.*"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    },
    "laravel": {
      "aliases": {
      },
      "providers": [
        "Nemerichun\\SzamlazzHu\\Providers\\SzamlazzHuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nemerichun/laravel-szamlazzhu.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}